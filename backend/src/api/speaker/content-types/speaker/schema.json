{
  "kind": "collectionType",
  "collectionName": "speakers",
  "info": {
    "singularName": "speaker",
    "pluralName": "speakers",
    "displayName": "Speaker",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Slug": {
      "type": "uid",
      "targetField": "Name",
      "required": true
    },
    "Title": {
      "type": "string",
      "required": true,
      "default": "Job Title"
    },
    "Organization": {
      "type": "string",
      "required": true,
      "default": "Current Org"
    },
    "ShortBio": {
      "type": "text",
      "required": true,
      "default": "A short description about the speaker"
    },
    "Bio": {
      "type": "text"
    },
    "Featured": {
      "type": "boolean",
      "default": false
    },
    "ProfileImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "LinkedIn": {
      "type": "string",
      "required": false
    },
    "Twitter": {
      "type": "string"
    },
    "Website": {
      "type": "string"
    },
    "sessions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::session.session",
      "mappedBy": "speakers"
    },
    "SortOrder": {
      "type": "integer"
    }
  }
}
